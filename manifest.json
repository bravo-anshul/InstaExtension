{
    "name": "InstaSearch",
    "version": "0.1",
    "description": "Tool used to perform a search on your story viewers on Instagram.",
    "manifest_version": 2,
    "browser_action":{
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/instaExtentionLogo_16.png",
        "48": "icons/instaExtentionLogo_48.png",
        "128": "icons/instaExtentionLogo_128.png"
    },
    "content_scripts": [{
        "css": ["style.css"],
        "js": ["content.js"],
        "matches": ["<all_urls>"]
    }]
}