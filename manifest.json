{
    "name": "try",
    "version": "0.1",
    "manifest_version": 2,
    "browser_action":{
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "css": ["style.css"],
        "js": ["content.js"],
        "matches": ["<all_urls>"]
    }],
    "background":{
        "scripts":["background.js"]
    },
    "permissions":["tabs"]
}